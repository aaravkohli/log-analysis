# Environment Variables for Log Sentinel Watcher
# Copy this file to .env.local for local development

# App Configuration
VITE_APP_NAME=Log Sentinel Watcher
VITE_APP_VERSION=1.0.0

# API Configuration (if you add backend services later)
# VITE_API_BASE_URL=https://api.yourdomain.com
# VITE_API_KEY=your_api_key_here

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false

# Security Settings
VITE_MAX_FILE_SIZE=10485760  # 10MB in bytes
VITE_ALLOWED_FILE_TYPES=.csv,.log,.txt

# GeoIP Configuration (if using external service)
# VITE_GEOIP_API_KEY=your_geoip_api_key
# VITE_GEOIP_SERVICE_URL=https://api.ipapi.com 